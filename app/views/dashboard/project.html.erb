<% content_for :header_tags do %>
  <%= stylesheet_link_tag 'dashboard', plugin: 'redmine_dashboard_pro' %>
  <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/Chart.js/3.9.1/chart.min.js' %>
  <%= javascript_include_tag 'dashboard', :plugin => 'redmine_dashboard_pro' %>
<% end %>
<div class="dashboard-container">
  <input type="hidden" value=<%=@project.id%> id='project_id'>
  <%= render "dashboard/project_header"%>
  <%= render "dashboard/header"%>
  <%= render "dashboard/overview"%>
  <div class="project-dashboard-grid" id="project-grids-wrapper">
    <% if User.current.allowed_to?(:view_issues, @project) %>
      <%= render "dashboard/project_issue_tracker"%>
    <% end %>
    <% if User.current.allowed_to?(:view_time_entries, @project) %>
      <%= render "dashboard/project_time_tracker"%>
    <% end %>
    <% if @principals_by_role.any? %>
      <%= render "dashboard/project_members_box" %>
    <% end %>
  </div>
  <%= render "dashboard/charts"%>
</div>